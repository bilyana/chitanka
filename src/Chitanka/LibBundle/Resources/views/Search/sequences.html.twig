{% extends 'LibBundle::layout.html.twig' %}

{% set title = 'Търсене' ~ (query ? ' на „'~query.text~'“' : '') ~ ' в издателските поредици' %}
{% block content %}

	{% if sequences|length %}
		{% include 'LibBundle:Sequence:sequences_list.html.twig' %}
	{% endif %}

	{% if not found %}
		<p class="no-items">Нямате късмет. Не открихме никакви издателски поредици.</p>
	{% endif %}

{% endblock %}
